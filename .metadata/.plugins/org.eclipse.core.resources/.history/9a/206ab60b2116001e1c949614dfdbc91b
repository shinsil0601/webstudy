<script type="text/javascript">
	document.write("오늘 날짜 : "+ new Date()+ "<br>");
	document.write("특정 날짜 : "+ new Date("2022-02-22")+ "<br>");
	document.write("특정 날짜 : "+ new Date(2022,04,26)+ "<br>");
	document.write("특정 날짜 및 시간 : "+ new Date(2022,04,26,09,10)+ "<br>");
</script>
