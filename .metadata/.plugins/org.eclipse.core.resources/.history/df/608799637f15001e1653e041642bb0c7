<script type="text/javascript">
	function mybtn() {
		// 이벤트 리스너
		// addEventListener("", function(){내용});
		document.getElementById("mybtn").addEventListener("click", function() {
			document.getElementById("demo1").innerHTML = new Date();
		});
	}
</script>
